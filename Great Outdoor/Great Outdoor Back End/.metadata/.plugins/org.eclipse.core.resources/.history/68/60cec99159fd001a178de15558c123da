package com.cg.go;

public class Main {

	public static void main(String[] args) {
	

		Person p=new Student();
		p.sayHello();
		
	}

}

 interface Person{
	
	void sayHello();
}

 class Student implements Person{
	 
	 String name;
	 
	 @Override
	public void sayHello() {
		System.out.println("helllloo");
		
	}
	 
 }